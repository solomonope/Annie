@model AnnieWeb.Models.FluidViewModel

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>FluidViewModel</legend>

            @Html.HiddenFor(model => model.FluidId)
        

        <div class="editor-label">
            @Html.LabelFor(model => model.FluidCode)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.FluidCode)
            @Html.ValidationMessageFor(model => model.FluidCode)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.FluidName)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.FluidName)
            @Html.ValidationMessageFor(model => model.FluidName)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
